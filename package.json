{
  "name": "scanner",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "rmdist": "rimraf dist",
    "build": "nest build",
    "build:api": "nest build api",
    "build:scan-engine": "nest build scan-engine",
    "build:cli": "nest build cli",
    "build:all": "concurrently \"npm run build:api\" \"npm run build scan-engine\" \"npm run build cli\"",
    "format": "prettier --write \"apps/**\" \"libs/**\"",
    "ingest": "nest start cli -- ingest",
    "scan": "nest start cli -- scan-site --url supremecourt.gov",
    "snapshot": "nest start cli -- create-snapshot",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:api": "nest start api",
    "start:scan-engine": "nest start scan-engine",
    "start:prod:api": "node dist/apps/api/main.js",
    "start:prod:scan-engine": "node dist/apps/scan-engine/main.js",
    "start:concurrent-scan-engine": "wait-on http://localhost:3000/api-json && nest start scan-engine",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:concurrent-scan-engine\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:unit": "jest --testPathIgnorePatterns e2e-spec",
    "test:e2e": "jest e2e-spec",
    "test:watch": "jest --watch --testPathIgnorePatterns e2e-spec",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.414.0",
    "@axe-core/puppeteer": "^4.8.5",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.10",
    "@nestjs/platform-express": "^9.3.10",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/swagger": "^6.2.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/json2csv": "^5.0.1",
    "bull": "^3.22.4",
    "class-transformer": "^0.3.1",
    "class-validator": ">=0.14.0",
    "commander": "^6.2.1",
    "cuid": "^2.1.8",
    "fast-csv": "^4.3.6",
    "generic-pool": "^3.8.2",
    "http-status-codes": "^2.2.0",
    "json2csv": "^5.0.7",
    "lodash": "^4.17.21",
    "nestjs-pino": "^3.1.2",
    "nestjs-typeorm-paginate": "^4.0.3",
    "pg": "^8.7.3",
    "pino-http": "^6.6.0",
    "puppeteer": "^13.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.2",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.3.14",
    "uuid": "^8.3.2",
    "webpack": ">=5.76.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^7.3.1",
    "@nestjs/testing": "^9.3.10",
    "@types/bull": "^3.15.7",
    "@types/express": "^4.17.11",
    "@types/generic-pool": "^3.1.10",
    "@types/html_codesniffer": "^2.5.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.178",
    "@types/node": "^14.18.10",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "concurrently": "^6.5.1",
    "eslint": "^7.25.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.3",
    "mermaid": "^9.3.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "sqlite3": "^5.1.5",
    "supertest": "^6.2.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^8.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.5.5",
    "wait-on": "^5.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testTimeout": 10000,
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/entities/",
      "<rootDir>/libs/"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "@app/message-queue/(.*)": "<rootDir>/libs/message-queue/src/$1",
      "@app/message-queue": "<rootDir>/libs/message-queue/src",
      "@app/database/(.*)": "<rootDir>/libs/database/src/$1",
      "@app/database": "<rootDir>/libs/database/src",
      "@app/browser/(.*)": "<rootDir>/libs/browser/src/$1",
      "@app/browser": "<rootDir>/libs/browser/src",
      "@app/core-scanner/(.*)": "<rootDir>/libs/core-scanner/src/$1",
      "@app/core-scanner": "<rootDir>/libs/core-scanner/src",
      "@app/ingest/(.*)": "<rootDir>/libs/ingest/src/$1",
      "@app/ingest": "<rootDir>/libs/ingest/src",
      "@app/storage/(.*)": "<rootDir>/libs/storage/src/$1",
      "@app/storage": "<rootDir>/libs/storage/src",
      "@app/snapshot/(.*)": "<rootDir>/libs/snapshot/src/$1",
      "@app/snapshot": "<rootDir>/libs/snapshot/src",
      "@app/datetime/(.*)": "<rootDir>/libs/datetime/src/$1",
      "@app/datetime": "<rootDir>/libs/datetime/src",
      "@app/queue": "<rootDir>/libs/queue/src",
      "@app/queue/(.*)": "<rootDir>/libs/queue/src/$1"
    }
  }
}
